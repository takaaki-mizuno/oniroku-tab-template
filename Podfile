target 'oniroku-template' do
    # Comment this line if you're not using Swift and don't want to use dynamic frameworks
    use_frameworks!
    
    # Pods for catalyst
    pod 'Kingfisher', '~> 3.1'
    pod 'Alamofire', '~> 4.0'
    pod 'ObjectMapper', '~> 2.1'
    pod 'SwiftyJSON', '~> 3.1'
    
    target 'oniroku-templateTests' do
        inherit! :search_paths
        # Pods for testing
    end
    
    target 'oniroku-templateUITests' do
        inherit! :search_paths
        # Pods for testing
    end
    
end
